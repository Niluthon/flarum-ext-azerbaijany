{
    "name": "niluthon/flarum-ext-azerbaijany",
    "description": "Flarum Azerbaijany Language Pack",
    "type": "flarum-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Niluthon",
            "email": "niluthon552@gmail.com"
        }
    ],

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Niluthon/flarum-ext-azerbaijany"
        }
    ],

    "require": {
        "flarum/core": "^0.1.0-beta.8"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "flarum-extension": {
            "title": "Azərbaycan",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#f3f6f9",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "az",
            "title": "Azərbaycan"
        }
    }
}
